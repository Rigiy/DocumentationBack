<div class="main wrap">
    <div class="left-side" id="navigation"></div>
    <div class="right-side">
        <div class="view" id="viewDoc">
            <% include ../_includes/sectionGenerate %>
            <div class="media-toolbar" id="media-toolbar">
                <span class="btn btn-toggle icon-plus"></span>
                <div class="menu-toolbar">
                    <span class="btn icon-image"></span>
                    <span class="btn icon-video"></span>
                    <span class="btn icon-code"></span>
                    <span class="btn icon-hr"></span>
                </div>
            </div>
            <div class="link" id="link-toolbar">
                <input class="link-val" type="text" value="" id="link_value" placeholder="Paste or type a link">
                <span class="close" id="link_close"></span>
            </div>
        </div>
    </div>
</div>